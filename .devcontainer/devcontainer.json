{
  "name": "{{gh-username}}/{{project-name}}",
  "image": "mcr.microsoft.com/vscode/devcontainers/rust:latest",
  "postCreateCommand": "bash scripts/postCreateCommand.sh",
  "forwardPorts": [8080],
  "appPort": [8080],
  "portsAttributes": {
    "8080": {
      "label": "web"
    }
  },
   "extensions": [
    "rust-lang.rust-analyzer"
  ]
}
